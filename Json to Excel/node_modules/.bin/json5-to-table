#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../json5-to-table/bin/json5-to-table" "$@"
else 
  exec node  "$basedir/../json5-to-table/bin/json5-to-table" "$@"
fi
